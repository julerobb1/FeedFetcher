#include "resource.h"

IDD_LOGIN_DIALOG DIALOGEX 0, 0, 200, 120
STYLE DS_SETFONT | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Login"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT "Username:", -1, 10, 10, 50, 10
    EDITTEXT IDC_USERNAME, 70, 10, 120, 12, ES_AUTOHSCROLL
    LTEXT "Password:", -1, 10, 40, 50, 10
    EDITTEXT IDC_PASSWORD, 70, 40, 120, 12, ES_PASSWORD | ES_AUTOHSCROLL
    DEFPUSHBUTTON "OK", IDOK, 50, 80, 50, 14
    PUSHBUTTON "Cancel", IDCANCEL, 110, 80, 50, 14
END
